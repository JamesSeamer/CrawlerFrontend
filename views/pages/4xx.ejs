<%- include('../partials/header', { clients: [], activeClientId: null }) %>

<h1>404 Errors</h1>

<h2>Internal 404s</h2>
<table id="Internal404sTable" class="display">
  <thead>
    <tr>
      <th>ID</th>
      <th>URL</th>
      <th>Content Type</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody>
    <% internal404s.forEach(url => { %>
      <tr>
        <td><%= url.id %></td>
        <td><a href="<%= url.url %>" target="_blank"><%= url.url %></a></td>
        <td><%= url.content_type %></td>
        <td><%= url.title %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<h2>External 404s</h2>
<table id="External404sTable" class="display">
  <thead>
    <tr>
      <th>ID</th>
      <th>URL</th>
      <th>Content Type</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody>
    <% external404s.forEach(url => { %>
      <tr>
        <td><%= url.id %></td>
        <td><a href="<%= url.url %>" target="_blank"><%= url.url %></a></td>
        <td><%= url.content_type %></td>
        <td><%= url.title %></td>
      </tr>
    <% }) %>
  </tbody>
</table>

<!-- jQuery + DataTables -->
<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
<script src="//cdn.datatables.net/2.3.3/js/dataTables.min.js"></script>
<script>
  $(document).ready(function() {
    $('#Internal404sTable').DataTable();
    $('#External404sTable').DataTable();
  });
</script>

<%- include('../partials/footer') %>
